<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- DOCKER -->
<mapper namespace="com.ssm.chapter.dao.DockerDao">

	<sql id="allColumns">
		ID, EVENTID, MODEL, IPADDR
	</sql>
	<!-- This code was generated by TableGo tools, mark 1 end. -->

	<!-- 插入数据 -->
	<insert id="insertDocker"
		parameterType="com.ssm.chapter.pojo.docker">
		INSERT INTO DOCKER (
		ID,
		EVENTID, MODEL, IPADDR,count
		)
		VALUES (
		#{id},
		#{eventid},
		#{model},
		#{ipaddr},
		#{count}
		)
	</insert>
	<!-- 根据主键查询数据 -->
	<select id="findDockerByIds"
		resultType="com.ssm.chapter.pojo.docker">
		SELECT
		<include refid="allColumns" />
		FROM DOCKER WHERE ID IN
		<foreach collection="list" index="index" item="id" open="("
			separator="," close=")">
			#{id}
		</foreach>
	</select>
	
</mapper>